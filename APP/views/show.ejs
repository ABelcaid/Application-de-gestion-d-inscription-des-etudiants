<body>

    </div>

    <div class="page-wrapper chiller-theme toggled">
        <a id="show-sidebar" class="btn btn-sm btn-dark" href="#">
            <i class="fas fa-bars"></i>
        </a>
        <nav id="sidebar" class="sidebar-wrapper">
            <div class="sidebar-content">
                <div class="sidebar-brand">
                    <a href="#">YourSchool</a>
                    <div id="close-sidebar">
                        <i class="fas fa-times"></i>
                    </div>
                </div>
                <div class="sidebar-header">
                    <div class="user-info">
                        <span class="user-name"><strong> Robert downey jr </strong></span>

                        <span class="user-role">Administrator</span>
                    </div>
                </div>
                <!-- sidebar-header  -->
            
                <div class="sidebar-menu">
                    <ul>
                        <li>
                            <a href="http://localhost:8080/AddStudent">
                                <i class="fa fa-book"></i>
                                <span>Add New student</span>
                            </a>
                        </li>
                        <li class="sidebar-dropdown">

                            <a href="#">
                                <i class="fas fa-graduation-cap"></i>
                                <span>List des niveau</span>
                            </a>
                            <div class="sidebar-submenu">
                                <ul>
                                    <li><a href="/Level/firstClass">firstClass</a></li>
                                    <li><a href="/Level/secondClass">secondClass</a></li>
                                    <li><a href="/Level/thirdClass">thirdClass</a></li>
                                    <li><a href="/Level/fourthClass">fourthClass</a></li>
                                    <li><a href="/Level/fifthClass">fifthClass</a></li>
                                    <li><a href="/Level/sixthClass">sixthClass</a></li>
                                    

                                </ul>
                            </div>
                        </li>


                    </ul>
                </div>
                <!-- sidebar-menu  -->
            </div>
            <!-- sidebar-content  -->

        </nav>
        <!-- sidebar-wrapper  -->
        <main class="page-content">
            <div class="container ">
                <h1 style="color: #f8f9fa;padding: 3%"><%=name%> Students List</h1>

                <div class="row ">
                    <div class="col-12 ">
                        <table class="table table-bordered " id="tab">
                            <thead>
                                <tr>
                                    <!-- <th scope="col">ID</th> -->
                                    <th scope="col">First Name</th>
                                    <th scope="col">Last Name</th>
                                    <th scope="col">Date of birth</th>
                                    <th scope="col">Gender</th>
                                    <th scope="col">Guardian's name</th>
                                    <th scope="col">Phone number</th>
                                    <th scope="col">Date of registration</th>
                                    <th scope="col">Actions</th>
                                </tr>
                            </thead>
                            <tbody>

                                <% for(i of currentList){ %>
                                <tr>

                                    <!-- <td><%= i.id %></td> -->
                                    <td><%= i.firstName %></td>
                                    <td><%= i.lastName %></td>
                                    <td><%= i.dateOfBirth %></td>
                                    <td><%= i.gender %></td>
                                    <td><%= i.guardian %></td>
                                    <td><%= i.phone %></td>
                                    <td><%= i.dateOfRegistration %></td>
                                    <td>
                                        <button type="button" class="btn btn-success" data-target="#exampleModal"><a class="a" href="/edit/Level/<%=name%>/<%=i.id %>"><i
                                                    class="fas fa-edit"></i></a></button>
                                        <button type="button" class="btn btn-danger" data-target="#exampleModal"><a class="a" href="/delete/Level/<%=name%>/<%=i.id %>"><i
                                                    class="far fa-trash-alt"></i></a></button>
                                    </td>
                                </tr>

                                <%}%>


                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </main>
        <!-- page-content" -->

    </div>
    
 
    </div>




